package com.xueersi.corelibs.event{	import flash.events.Event;	import com.xueersi.corelibs.interfaces.IVo;	/**	 * @author 网上收集	 * @time 	2011-8-10 18:43	 */	public class TimelineEvent extends Event	{		/** 到达帧 */		public static const LABEL_REACHED:String="labelReached";		/** 到达影片剪辑的结尾 */		public static const END_REACHED:String="endReached";		private var _currentFrame:int;		private var _currentFrameLabel:String;		private var _data:IVo;		/*		 * The TimelineEvent class provides a custom Event for the com.refunk.timeline.TimelineWatcher class		 *		 * It provides two String constants named LABEL_REACHED and END_REACHED,		 * and can hold a timeline's current frame and label to be passed by the event object		 */		public function TimelineEvent(type:String, currentFrame:int=0, currentFrameLabel:String=null, data:IVo=null)		{			super(type);			_currentFrame=currentFrame;			_currentFrameLabel=currentFrameLabel;			_data=data;		}		public override function clone():Event		{			return new TimelineEvent(type, _currentFrame, currentFrameLabel,_data);		}		public override function toString():String		{			return formatToString("TimelineEvent", "type", "bubbles", "cancelable", "eventPhase", "currentFrame", "currentFrameLabel","data");		}		/*		 * Getter function		 *		 * @return 	The current frame (int)		 */		public function get currentFrame():int		{			return _currentFrame;		}		/*		 * Getter function		 *		 * @return 	The current label (String)		 */		public function get currentFrameLabel():String		{			return _currentFrameLabel;		}		public function get data():IVo		{			return _data;		}	}}